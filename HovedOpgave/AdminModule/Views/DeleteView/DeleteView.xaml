<Window x:Class="AdminModule.Views.DeleteView.DeleteView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="DeleteView" Height="456.717" Width="477.612" Closing="Window_Closing_1">
        <Grid Background="CadetBlue">
        <ComboBox HorizontalAlignment="Left" Margin="10,74,0,0" VerticalAlignment="Top" Width="120" ItemsSource="{Binding ListofClassEx}" SelectedItem="{Binding SelectedClassEx}">
        </ComboBox>
        <ListBox HorizontalAlignment="Left" Height="249" Margin="215,74,0,0" VerticalAlignment="Top" Width="245" ItemsSource="{Binding CurrentClass.StudentsList}"/>
        <Button Content="Tilknyt" HorizontalAlignment="Left" Margin="135,74,0,0" VerticalAlignment="Top" Width="75"/>
        <Button Content="Annuller" HorizontalAlignment="Left" Margin="385,396,0,0" VerticalAlignment="Top" Width="75"/>
        <Button Content="Bekræft" HorizontalAlignment="Left" Margin="305,396,0,0" VerticalAlignment="Top" Width="75" Command="{Binding ConfirmCommand}"/>




    </Grid>
</Window>
